<h1><%= @user.email %></h1>
<h3>Name: <%= "#{@user.first_name} #{@user.last_name}" %></h3>
<%= form_for @user, url: {action: "update"}, html: {class: "edit-user"} do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.submit "Update" %>
<% end %>